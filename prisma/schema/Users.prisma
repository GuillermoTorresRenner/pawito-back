model Users {
    userID         String    @id @default(uuid())
    email          String    @unique
    password       String
    name           String
    surname        String
    address        String
    role           Role      @default(USER)
    lastConnection DateTime?
    createdAt      DateTime  @default(now())
    updatedAt      DateTime  @updatedAt
    deletedAt      DateTime?
}

enum Role {
    ADMIN
    USER
}
